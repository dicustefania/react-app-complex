{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
	{
	  "name": "redis",
	  "image": "redis",
	  "hostname": "redis",
	  "essential": false,
	  "memory": 128
	},
	{
	  "name": "postgres",
	  "image": "postgres",
	  "hostname": "postgres",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "client",
	  "image": "stefaniadicu/complex-client",
	  "hostname": "client",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "server",
	  "image": "stefaniadicu/complex-server",
	  "hostname": "api",
	  "essential": false,
	  "memory": 128,
	  "links": ["redis","postgres"]
	},
    {
	  "name": "worker",
	  "image": "stefaniadicu/complex-worker",
	  "hostname": "worker",
	  "essential": false,
	  "memory": 128,
	  "links": ["redis"]
	},
	{
	  "name": "nginx",
	  "image": "stefaniadicu/complex-nginx",
	  "essential": true,
	  "portMappings": [{"hostPort": 80,"containerPort": 80}],
	  "links": ["client","server"],
	  "memory": 128
	}
 ]
  
}